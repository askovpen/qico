#include <config.h>
#include <stdio.h>
#ifdef HAVE_STDLIB_H
#include <stdlib.h>
#endif
#include <sys/utsname.h>
#include "ver.h"
#include "cvsdate.h"

char progname[]="@PACKAGE_NAME@";
char version[]="@PACKAGE_VERSION@";
char *osname="Unix";

void u_vers(char *progn)
{
	struct utsname uts;
	printf("%s-%s %s",progn,version,cvsdate);
	if(!uname(&uts))printf(" on %s-%s (%s)",uts.sysname,uts.release,uts.machine);
	printf("\ncompiled: " __DATE__ " " __TIME__ ", cc: " __VERSION__ "\n");
	exit(0);
}
